# Backend Environment Variables Template
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit .env file to Git!

# Database Configuration
DB_URL=jdbc:mysql://localhost:3306/your_database_name
DB_USERNAME=your_db_username
DB_PASSWORD=your_db_password

# Email Configuration (Gmail App Password)
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_gmail_app_password

# JWT Configuration (Generate a strong random string)
JWT_SECRET=your_jwt_secret_key_here_minimum_256_bits

# VNPay Configuration (Get from VNPay Merchant Portal)
VNPAY_TMN_CODE=your_vnpay_tmn_code
VNPAY_SECRET_KEY=your_vnpay_secret_key
VNPAY_RETURN_URL=http://localhost:5454/api/payment/vnpay/return
